plot(WorldHungerEuropeAndCentralAsia$`Severe Wasting`~factor(WorldHungerEuropeAndCentralAsia$Countries), xlab = "Countries", ylab = "Severe Wasting Percentage", xaxt="none",main="Europe and Central Asian Region")
axis(1,seq(0,21,1),las=2)
plot(WorldHungerEuropeAndCentralAsia$Wasting~factor(WorldHungerEuropeAndCentralAsia$Countries), xlab = "Countries",  ylab = "Wasting Percentage", xaxt="none",main="Europe and Central Asian Region")
axis(1,seq(0,21,1),las=2)
