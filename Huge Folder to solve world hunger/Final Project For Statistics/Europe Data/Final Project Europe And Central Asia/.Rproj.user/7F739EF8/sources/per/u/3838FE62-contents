axis(1,seq(0,21,1),las=2)
plot(WorldHungerEuropeAndCentralAsia$Stunting~factor(WorldHungerEuropeAndCentralAsia$Countries), xlab = "Countries", ylab = "Stunting Percentage", xaxt="none",main="Europe and Central Asian Region")
axis(1,seq(0,21,1),las=2)
