plot(WorldHungerLatinAmericaAndCaribbeans$`Severe Wasting`~factor(WorldHungerLatinAmericaAndCaribbeans$Countries), xlab = "Countries", ylab = "Severe Wasting Percentage",xaxt="none",main="Latin America and the Caribbeans")
axis(1,seq(0,27,1),las=2)
