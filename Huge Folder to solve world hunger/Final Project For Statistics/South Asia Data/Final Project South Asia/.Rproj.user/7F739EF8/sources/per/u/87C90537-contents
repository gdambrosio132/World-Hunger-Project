plot(WorldHungerSouthAsia$`Severe Wasting`~factor(WorldHungerSouthAsia$Countries), xlab = "Countries", ylab = "Severe Wasting Percentage",xaxt="none",main="South Asian Region")
axis(1,seq(0,8,1),las=2)
