plot(WorldHungerSubSaharanAfrica$`Severe Wasting`~factor(WorldHungerSubSaharanAfrica$Countries), xlab = "Countries", ylab = "Severe Wasting Percentage",xaxt="none",main="Sub-Saharan African Region")
axis(1,seq(0,48,2),las=2)
